
.diamond {
	width: 100%;
	height: 100%;
	box-sizing: border-box;
	padding: 10rpx 20rpx;
	display: flex;
	flex-wrap: wrap;
	justify-content: space-around;
}
.dl {
	width: 160rpx;
	height: 160rpx;
	padding: 5rpx 15rpx;
	box-sizing: border-box;
}
.dt {
	width: 120rpx;
	height: 120rpx;
}
.dt ._img {
	width: 100%;
	height: 100%;
	object-fit: contain;
}
.dd .p {
	text-align: center;
	font-size: 28rpx;
	color: #000;
}

