<view><view class="search_bar"><view data-event-opts="{{[['tap',[['toggleDrop',['$event']]]]]}}" class="search" bindtap="__e"><view class="dropdown-container"><view data-event-opts="{{[['tap',[['toggleDropdown',['$event']]]]]}}" class="dropdown-header" bindtap="__e"><view class="titleContent">{{selected}}</view><label class="_span"></label></view><block tt:if="{{isOpen}}"><view class="dropdown-list"><block tt:for="{{options}}" tt:for-item="option" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['selectOption',['$0'],[[['options','',index]]]]]]]}}" class="li" bindtap="__e">{{''+option+''}}</view></block></view></block></view><input placeholder-style="color:#999; font-size:24rpx;" type="text" placeholder="请输入" data-event-opts="{{[['input',[['__set_model',['','searchBarTitle','$event',[]]]]]]}}" value="{{searchBarTitle}}" bindinput="__e"/><image src="{{$root.m0}}" alt data-event-opts="{{[['tap',[['searchType',['$event']]]]]}}" bindtap="__e" class="_img"></image></view><image class="jia _img" src="{{$root.m1}}" alt data-event-opts="{{[['tap',[['jiaClick',['$event']]]]]}}" bindtap="__e"></image></view><view class="compontents"><view class="titleTop">食品好帮手 > 搜索</view><view class="tab-bar_t"><view data-event-opts="{{[['tap',[['GetProductListClick',['$event']]]]]}}" class="{{(('tab_t')+' '+((activeTab===23)?'active_t':''))}}" bindtap="__e">产品</view><view data-event-opts="{{[['tap',[['detailClickShopClick',['$event']]]]]}}" class="{{(('tab_t')+' '+((activeTab===22)?'active_t':''))}}" bindtap="__e">公司</view></view><view class="tab-content_t"><view hidden="{{!(activeTab===23)}}"><scroll-view class="shop_p_" scroll-y="{{true}}" data-event-opts="{{[['scrolltolower',[['GetProductList',['$event']]]]]}}" bindscrolltolower="__e"><view class="shop_type"><block tt:for="{{tabCont}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['typeDetil',['$0'],[[['tabCont','',index]]]]]]]}}" class="img_cont" bindtap="__e"><view class="img_big"><image src="{{item.procPic}}" alt class="_img"></image></view><view class="img_text">{{''+item.procTitle+''}}</view></view></block></view></scroll-view></view><view hidden="{{!(activeTab===22)}}"><scroll-view class="shop_p_" scroll-y="{{true}}" data-event-opts="{{[['scrolltolower',[['detailClickShop',['$event']]]]]}}" bindscrolltolower="__e"><view class="shopListDetial"><block tt:for="{{tabContTitle}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="shop_listDetial"><view data-event-opts="{{[['tap',[['detailClick',['$0'],[[['tabContTitle','',index,'userId']]]]]]]}}" bindtap="__e"><view class="listDetial">{{''+item.companyName+''}}</view><view class="list_operateDetial"><view class="operateDetial">{{'主营：'+item.proTypeName+''}}</view><view class="addressDetial">{{''+item.companyAddress+''}}</view></view><view class="list_vipDetial"><view class="vipDetial"><view hidden="{{!(item.yearcount=='0')}}" class="vipNew">1年</view><view hidden="{{!(item.yearcount!='0')}}" class="vipNew">{{''+item.yearcount+'年'}}</view></view><view hidden="{{!(item.isShili>0)}}" class="solidDetial fs">实</view><view hidden="{{!(item.isTuijian==1)}}" class="pushDetial fs">推</view></view></view><view class="swiper_conDetial"><view class="swiper_imgDetial"><block tt:for="{{item.produceInfoModel}}" tt:for-item="items" tt:for-index="indexs" tt:key="indexs"><view data-event-opts="{{[['tap',[['ShopDetailClick',['$0'],[[['tabContTitle','',index],['produceInfoModel','',indexs]]]]]]]}}" class="imgSwiperDetial" bindtap="__e"><image src="{{items.procPic}}" alt class="_img"></image></view></block></view></view></view></block></view></scroll-view></view></view></view></view>