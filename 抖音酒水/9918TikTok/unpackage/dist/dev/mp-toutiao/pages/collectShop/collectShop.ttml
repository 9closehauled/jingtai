<view><view class="collectCompontent"><block tt:for="{{shopListContent}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="shop_cont"><view class="shop_content"><image src="{{item.imagePath}}" mode="widthFix" data-event-opts="{{[['tap',[['shopClickDiff',['$0'],[[['shopListContent','',index,'userId']]]]]]]}}" bindtap="__e"></image><view data-event-opts="{{[['tap',[['shopClickDiff',['$0'],[[['shopListContent','',index,'userId']]]]]]]}}" class="shopTitle" bindtap="__e"><view class="shopName">{{''+item.companyName+''}}</view><view class="shop_honor"><view class="vipDetial"><block tt:if="{{item.yearcount>0}}"><view class="vipNew">{{''+item.yearcount+'年'}}</view></block><block tt:else><view class="vipNew">1年</view></block></view><view hidden="{{!(item.isShili>0)}}" class="solidDetial fs">实</view><view hidden="{{!(item.isTuijian==1)}}" class="pushDetial fs">推</view></view><view class="operateDetial">{{'主营：'+item.proTypeName+''}}</view></view><view data-event-opts="{{[['tap',[['collectClick',['$0'],[[['shopListContent','',index,'id']]]]]]]}}" class="btnRed" bindtap="__e">取消收藏</view></view></view></block><view class="title_bar">没有更多数据了</view></view></view>